package daelim.uml;

public class Marine {
	
	private String Name;
	public int Health;
	protected int AttackPower;
	
	public String getName() {
		return Name;
	}

	public void setName(String name) {
		Name = name;
	}

	private void Move() {
		System.out.println("마린 이동한다.");
	}
	
	public void Attack() {
		System.out.println("마린 공격한다.");
	}
	
	public void UnderAttack(char c1) {
		Move();
		System.out.println("마린 위치로 공격하러간다.");
	}
}
